  "license": "ISC",
  "description": "",
  "dependencies": {
    "cypress": "^13.15.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "cypress-xpath": "^2.0.1"
  }
}
